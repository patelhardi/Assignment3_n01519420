@model IEnumerable<Assignment3_n01519420.Models.Teacher>
@{
    ViewBag.Title = "List";
}

<div id="containerlist">

    <h2>List of Teachers</h2>

    @foreach (var teacher in Model)
    {
        <div class="listitem row">
            <div class="col-md-12"><a href="/Teacher/Show/@teacher.TeacherId">@teacher.TeacherFName @teacher.TeacherLName @teacher.TEmpNumber</a></div>
        </div>
    }

    <a class="navlink" href="/Teacher/Add">Add New Teacher</a>
</div>
